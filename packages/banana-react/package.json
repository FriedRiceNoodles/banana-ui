{
  "name": "@banana-ui/react",
  "version": "1.17.0",
  "description": "React components for Banana UI",
  "keywords": [
    "web components",
    "custom elements",
    "components",
    "UI library",
    "react"
  ],
  "homepage": "https://github.com/FriedRiceNoodles/banana-ui",
  "license": "MIT",
  "author": "炒米粉",
  "type": "module",
  "exports": {
    ".": {
      "worker": "./node/index.js",
      "node": "./node/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index",
  "module": "dist/index",
  "files": [
    "dist/*",
    "umd/*",
    "node/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rimraf umd && rimraf dist && rimraf node && rollup -c ../../rollup.config.mjs && npm run tsc",
    "dev": "rollup -c ../../rollup.config.dev.mjs -p ../../scripts/rollup-plugin-copy-types.js -w --no-watch.clearScreen",
    "tsc": "rimraf types && npx tsc --project ./tsconfig.types.json && node ./copyTypesToDist.cjs"
  },
  "dependencies": {
    "@banana-ui/banana": "workspace:*",
    "@lit-labs/react": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "registry": "https://www.npmjs.com/package/@banana-ui/react"
  },
  "moduleResolution": "node"
}
